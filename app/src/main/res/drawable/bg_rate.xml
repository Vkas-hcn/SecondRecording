<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">

            <corners android:radius="3dp"/>

            <gradient
                android:type="linear"
                android:startColor="#09B891"
                android:endColor="#64F01C"
                android:angle="270" />

        </shape>
    </item>

    <item
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"
        android:bottom="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <solid android:color="#F9FFFB"/>
        </shape>
    </item>
</layer-list>
